 // Rotate a matrix by 90 degrees
#include <iostream>
using namespace std;

int main(){
   
    int n;
    cout<<"Enter n: ";
    cin>>n;
    int arr[n][n];
    cout<<"Enter elements: ";
    for(int i=0; i<n; i++){
        for(int j=0; j<n; j++){
            cin>>arr[i][j];
        }
    }
    cout<<"The original matrix is: "<<endl;
    for(int i=0; i<n; i++){
        for(int j=0; j<n; j++){
            cout<<arr[i][j]<<" ";
        }
        cout<<endl;
    }
    cout<<"The rotated matrix is: "<<endl;
    for(int i=0; i<n; i++){
        for(int j=n-1; j>=0; j--){
            cout<<arr[j][i]<<" ";
        }
        cout<<endl;
    }
    
}
